//삼각형 종류2
/*
float tlength(int ax, int ay, int bx, int by);
void compar(float base, float height, float hypo);
void compar2(float base, float height, float hypo);

int main()
{
	int ax, ay, bx, by, cx, cy;
	cin >> ax >> ay >> bx >> by >> cx >> cy;

	float base, height, hypo;
	base = tlength(ax, ay, bx, by);
	height = tlength(ax, ay, cx, cy);
	hypo = tlength(bx, by, cx, cy);

	if (base >= height && base >= hypo)
		compar2(base, height, hypo);
	else if (height >= base && height >= hypo)
		compar2(height, base, hypo);
	else
		compar2(hypo, height, base);

}

float tlength(int ax, int ay, int bx, int by) {
	return sqrt(pow(ax - bx, 2) + pow(ay - by, 2));
}

void compar(float base, float height, float hypo)
{

	if (pow(base, 2) == pow(height, 2) + pow(hypo, 2))
		cout << "1";
	else if (pow(base, 2) > pow(height, 2) + pow(hypo, 2))
		cout << "2";
	else
		cout << "3";
}

void compar2(float base, float height, float hypo)
{
	if (base >= height + hypo)
		cout << "0";
	else
		compar(base, height, hypo);
}*/



//삼각형 종류 3
/*
int squareis(int x, int x1, int y , int y1, int x2, int y2) {
	if (x >= x1 && x <= x2 && y >= y1 && y <= y2)
		return true;
	else
		return false;
}
void outsquare(int x, int x1, int y, int y2) {
	cout << pow((x - x1), 2) + pow((y - y2), 2) << " " << abs(x - x1) + abs(y - y2);
}

void insquare(int x, int x1, int x2) {
	if (x > x2)
		cout << pow((x - x2), 2) << " " << x - x2;
	else
		cout << pow((x - x1), 2) << " " << abs(x1 - x);
	
}
int main()
{
	int x1, x2, y1, y2;
	cin >> x1 >> y1 >> x2 >> y2; //사각형 위치
	int x, y;
	cin >> x >> y; //점의 좌표
	

	if (squareis(x, x1, y, y1, x2, y2) == 1)//사각형 안
		cout << "0 0";

	while (squareis(x, x1, y, y1, x2, y2) == 0) {
	if (y >= y1 && y <= y2)
	{
		insquare(x, x1, x2);
		break;
	}
	else if (x >= x1 && x <= x2)
	{
		insquare(y, y1, y2);
		break;
	}
	else if (x > x2 && y > y2)//1
	{
		outsquare(x, x2, y, y2);
		break;
	}
	else if (x<x1 && y>y2)//2
	{
		outsquare(x, x1,  y, y2);
		break;
