/*
double f_abs(double x) {
	if (x < 0)
		return -x;
	else
		return x;
}
double f_min(double x, double y) {
	if (x < y)
		return x;
	else
		return y;
}
double f_equal(double x, double y) {
	double e = 0.000001;
	if (f_abs(x - y) < e * f_min(f_abs(x), f_abs(y)))
		return 1;
	else
		return 0;

}

int main()
{
	double a, b;
	cout << "두 실수를 입력하시오 :";
