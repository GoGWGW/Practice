//starplus
/*
int main(void)
{
	int size;
	cin >> size;

	int center, starPlus, minus;

	for (int row = 1; row <= size; row++)
	{
		int i, line = row;
		center = size / 2 + 1;
		if (row > center)
			line = 2 * center - row;

		starPlus = size - (line - 1) * 2;
		minus= (size - starPlus) / 2;

		for (i = 0; i < minus; i++)
			cout << "-";
		for (i = 0; i < starPlus; i++)
			if (i % 2 == 0)
				cout << "*";
			else
				cout << "+";

		for (i = 0; i < minus; i++)
			cout << "-";
		cout << endl;
	}
	
}*/


