void con(int n)
{
	for (int i = 1; i <= n; i++)
	{
		if (i % 2 == 1)
		{
			for (int j = n * (i - 1) + 1; j < n * (i - 1) + 1 + n; j++)
				printf("%3d", j);
			cout << endl;
		}
		else
		{
			for (int j = n * i; j > n * i - n; j--)
				printf("%3d", j);
			cout << endl;
		}
