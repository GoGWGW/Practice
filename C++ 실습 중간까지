//april 9th 실습 1번
/*#define SIZE 10

void printArray(int* A)
{
	for (int i = 0; i < SIZE; i++)
		printf("[%d] ", *(A + i));
}

void makeArray(int* A)
{
	for (int i = 0; i < SIZE; i++)
		* (A + i) = rand() % 100;
}

void swapArray(int* A, int* B)
{
	int n[SIZE];
	for(int i=0;i<SIZE;i++)
	{
		n[i] = rand() % SIZE;
		for (int j = 0; j < i; j++)
		{
			if (n[j] == n[i])
			{
				i--;
				break;
			}
		}
		*(B + n[i]) = *(A + i);
	}

	printArray(B);
}

int main()
{
	srand(time(NULL));
	int arr[SIZE];
	int arr2[SIZE];

	makeArray(arr);
	cout << "배열 A : "; printArray(arr);
	cout << endl << "배열 A (SWAP) : "; swapArray(arr, arr2);
}*/



//april 9th 2
/*
typedef struct point {
	int x;
	int y;
}point;

int calArea(point p[],int n) {
	int area = 0;

	p[n].x = p[0].x;
	p[n].y = p[0].y;

	for (int i = 0; i < n; i++)
		area += (p[i].x + p[i + 1].x) * (p[i + 1].y - p[i].y);
	return area;
}
int main()
{
	point p[10];
	int area,n;
	cin >> n;

	for(int i=0;i<n;i++)
		cin >> p[i].x >> p[i].y;


	area = calArea(p,n);


	cout << (area > 0 ? area : -area) << ' ' << (area > 0 ? 1 : -1) << endl;
}*/

//april 9 num 1
/*
typedef struct point {
	int x;
	int y;
}point;

int calArea(point a, point b, point c) {
	return (b.x - a.x) * (c.y - a.y) - (c.x - a.x) * (b.y - a.y);
}
int main()
{
	point a,b,c;
	int area;

	cin >> a.x >> a.y >> b.x >> b.y >> c.x >> c.y;
	area = calArea(a, b, c);

	int d;
	if (area > 0)
		d = 1;
	else if (area < 0)
	{
		d = -1;
		area = -area;
	}
	else
		d = 0;
	
	cout << area << ' ' << d << endl;
}
*/



